@{
    ViewData["Title"] = "DigitalCredential Details";
}
@model OH.DI.Web.ViewModels.DigitalCredentialViewModel
<h2>DigitalCredential (MVC View)</h2>

<h3>Name: @Model?.Name</h3>

<b>Items:</b>

<ul>
    @if (Model?.Items != null)
    {
        @foreach (var item in Model.Items)
        {
            <li>@item.Title<br/>@item.Description</li>
        }
    }
</ul>
